{
	"manifest_version": 2.0,
	"id": "latestdataconnector",
	"type": "widget",
	"name": "Latest data connector",
	"namespace": "LatestDataConnector",
	"description": "Enables one-click navigation to the Latest data page with pre-populated filters",
	"version": "1.0",
	"author": "Ryan Eberly",
	"actions": {
		"widget.latestdataconnector.view": {
			"class": "WidgetView"
		}
	},
	"widget": {
		"name": "LatestDataConnector",
		"size": {
			"width": 4,
			"height": 3
		},
		"refresh_rate": 0,
		"js_class": "CWidgetLatestDataConnector"
	},
	"assets": {
		"css": ["widget.css"],
		"js": ["class.widget.js"]
	}
}
